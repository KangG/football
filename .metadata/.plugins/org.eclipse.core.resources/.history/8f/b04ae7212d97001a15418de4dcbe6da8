package kim.kang.kitri.view.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

import kim.kang.kitri.post.PostVO;
import kim.kang.kitri.post.impl.PostDAO;

@Controller
public class GetPostListController {

	
	@RequestMapping("/getPostList.do")
	public ModelAndView getPostList(PostVO vo, PostDAO postDAO, ModelAndView mav)
	{
		mav.addObject("getlist",postDAO.getPostList(vo));
		mav.setViewName("PostList.jsp");
		
		return mav;
	}
	
}
