package kim.kang.kitri.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;


@Controller
public class PostController {


	@RequestMapping("/getPostList.do")
	public ModelAndView getPostList(PostVO vo, PostDAO postDAO, ModelAndView mav)
	{
		mav.addObject("getlist",postDAO.getPostList(vo));
		mav.setViewName("PostList.jsp");
		
		return mav;
	}
}
